<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans.xsd 
		http://www.springframework.org/schema/context 
		http://www.springframework.org/schema/context/spring-context.xsd">

  <context:component-scan base-package="com.eulersbridge.iEngage" />
  <bean id="testLogin" class="com.eulersbridge.iEngage.core.domain.Login">
  	<constructor-arg value="0"/>
  	<constructor-arg value="testLogin"/>
  	<constructor-arg value="testLogin tested"/>
  </bean>
  
  <bean id="countryFactory" class="com.eulersbridge.iEngage.rest.domain.stubCountryFactory" >
  </bean>
  
</beans>
